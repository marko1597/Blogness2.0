<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="BlogApi" value="https://localhost/blogapi/api/" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
    <add key="BlogRoot" value="https://localhost/blog/" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
    <add key="SessionCookieName" value="xremote" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>